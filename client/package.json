    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/stats-lite": "2.2.2",
    "@vitejs/plugin-react-swc": "3.6.0",
    "cypress": "13.7.0",
    "eslint-plugin-cypress": "2.15.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "react-scripts": "5.0.1",
    "tailwindcss": "3.4.1",
    "typescript": "4.9.5",
    "vite": "5.1.6"
