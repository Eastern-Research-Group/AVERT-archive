    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "stats-lite": "2.2.0",
    "web-vitals": "3.5.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,html}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
