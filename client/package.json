    "@testing-library/user-event": "12.1.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-redux": "7.1.9",
    "@types/stats-lite": "2.2.0",
    "cypress": "5.0.0",
    "eslint-plugin-cypress": "2.11.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.13",
    "prettier": "2.1.1",
    "react-scripts": "4.0.3",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
